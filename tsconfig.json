{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "baseUrl": "src/",
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["dom"],
    "moduleResolution": "node",
    "module": "commonjs",
    "outDir": "dist",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es5"
  },
  "include": ["src/**/*"],
  "exclude": ["test", "examples"]
}
